{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "auto-imports.d.ts",
    "components.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "#build/ui/*": ["./node_modules/.nuxt-ui/ui/*"],
      "@/*": ["src/*"]
    },
    "types": ["@modyfi/vite-plugin-yaml/modules"]
  }
}
